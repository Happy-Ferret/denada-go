package main

type Denada Peg {
	 Ids []string;
}

File <- ( ( Definition ) Spacing )* !.

Definition <- (Id Spacing)* Id '{' File '}'

Declaration <- (Id Spacing)* Id ';'

Id <- <[a-zA-Z0-9]+> {
   fmt.Printf("ID!\n");
   p.Ids = append(p.Ids, buffer[begin:end]);
}

Spacing <- Space*

Space <- ' ' / '\t'
